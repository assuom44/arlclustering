Package: arlclustering
Type: Package
Title: ARL Clustering Method for Social Network Analysis
Version: 1.0.0
Authors@R: c(
    person("Mohamed", "El-Moussaoui", email = "med.elmoussaoui.ced@gmail.com", role = c("aut", "cre")),
